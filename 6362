local Tab1 = Window:CreateTab("Visuals", 4483362458) -- Title, Image
local Section = Tab1:CreateSection("Ambient")
local Toggle = Tab1:CreateToggle({
    Name = "Ambient",
    CurrentValue = false,
    Callback = function(bool)
    getgenv().Ambient = bool
    game:GetService("Lighting").Ambient = Color3.fromRGB(255,250,250)
	if getgenv().Ambient == false then 
	game:GetService("Lighting").Ambient = Color3.fromRGB(0,0,0)
	end
	end
 })
